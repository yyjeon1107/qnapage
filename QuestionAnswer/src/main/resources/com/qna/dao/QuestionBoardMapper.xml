<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qna.dao.QuestionBoardMapper">


<insert id="save"  keyProperty="question_board_id">
	insert into questionboard (title, content)
	values (#{title}, #{content})
</insert>

<select id="findAll" resultType="QuestionBoard">
	select question_board_id, title, content, reg_dtime from 
	questionboard
</select>



</mapper>